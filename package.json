{
  "name": "earncro-xyz",
  "version": "0.0.0",
  "scripts": {
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve",
    "dev:full": "concurrently --names 'DEV,FUNCTIONS' --prefix-colors 'yellow,blue' --kill-others 'npm run dev' 'cd functions && npm run serve'"
  },
  "dependencies": {
    "@tailwindcss/forms": "0.4.0",
    "@tailwindcss/typography": "0.5.0",
    "localforage": "1.10.0",
    "mitt": "3.0.0",
    "tailwindcss": "3.0.15",
    "vue": "3.2.27",
    "vue-router": "4.0.12"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.16.3",
    "@vue/cli-plugin-babel": "~5.0.0-rc.2",
    "@vue/cli-plugin-eslint": "~5.0.0-rc.2",
    "@vue/cli-plugin-unit-jest": "~5.0.0-rc.2",
    "@vue/cli-service": "~5.0.0-rc.2",
    "@vue/compiler-sfc": "^3.2.6",
    "autoprefixer": "10.4.2",
    "eslint": "8.7.0",
    "eslint-plugin-vue": "8.0.3",
    "jest": "^27.1.0",
    "postcss": "8.4.5",
    "postcss-import": "14.0.2",
    "postcss-loader": "6.2.1",
    "typescript": "4.4.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "ecmaVersion": 2018,
      "requireConfigFile": false
    },
    "rules": {
      "vue/custom-event-name-casing": 0,
      "vue/multi-word-component-names": 0
    },
    "globals": {
      "defineProps": "readonly",
      "defineEmits": "readonly"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
